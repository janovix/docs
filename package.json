		"tw-animate-css": "^1.4.0",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.15",
		"vitest-axe": "^0.1.0",
		"wrangler": "4.51.0"
	},
	"private": true,
	"scripts": {
		"build": "opennextjs-cloudflare build",
		"next-build": "next build",
		"cf-build": "opennextjs-cloudflare build",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"prepare": "husky",
		"typecheck": "tsc --noEmit",
		"check": "pnpm run next-build && pnpm run typecheck",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"dev": "next dev",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint . --max-warnings=0",
		"lint:fix": "eslint . --fix",
		"test": "vitest run --coverage",
		"test:unit": "vitest run",
		"test:watch": "vitest",
		"ci:check": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run test",
		"predeploy:preview": "pnpm run cf-build",
		"versions:upload": "node ./scripts/versions-upload.mjs",
		"storybook": "pnpm exec storybook dev -p 6006",
		"build-storybook": "node scripts/build-storybook.mjs",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"release": "semantic-release",
		"start": "next start"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,md,mdx,json,yml,yaml,css,scss}": [
			"prettier --write",
			"eslint --fix"
		]
	}
}
